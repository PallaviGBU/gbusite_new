@model IEnumerable<gbuweb.Models.Program>

@{
    Layout = null;
}

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                <a href="/admission/program/@Html.DisplayFor(modelItem => item.id)"> Details</a>
            </td>
        </tr>
    }

</table>

